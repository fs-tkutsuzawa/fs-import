{
  "name": "financial-model-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^17.2.2",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-data-grid": "^7.0.0-beta.46",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm:start\" \"npm:server\"",
    "server": "cd server && npm start",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report docs/test-artifacts/playwright-report",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src server/src",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx src server/src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "inject:super-calc": "node --experimental-strip-types scripts/cli/inject-super-calc.ts",
    "show:rules": "node --experimental-strip-types scripts/cli/show-calculation-rules.ts",
    "sync:ua": "node --experimental-strip-types scripts/cli/sync-user-accounts.ts",
    "rules:sync-codes": "node --experimental-strip-types scripts/cli/sync-master-rules-ga-code.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "next": "^15.5.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.1"
  }
}
